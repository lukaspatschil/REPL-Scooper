{
  "name": "repl-scooper",
  "displayName": "REPL Scooper",
  "description": "A methode to experiment with code snippets",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.48.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.replscooper",
    "onCommand:extension.replcommand"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.replscooper",
        "title": "Open REPL Window",
        "category": "REPL Scooper"
      },
      {
        "command": "extension.replcommand",
        "title": "Open REPL Command",
        "category": "REPL Scooper"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "npm-run-all compile:*",
    "lint": "eslint src --ext ts",
    "watch": "npm-run-all -p watch:*",
    "pretest": "yarn run compile && yarn run lint",
    "test": "node ./out/test/runTest.js",
    "compile:extension": "tsc -p ./",
    "compile:views": "webpack --mode development",
    "watch:extension": "tsc -watch -p ./",
    "watch:views": "webpack --watch --mode development",
    "postinstall": "rm -rf node_modules/@types/astring/node_modules"
  },
  "devDependencies": {
    "@types/acorn": "^4.0.5",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.14.20",
    "@types/react": "^16.9.55",
    "@types/react-dom": "^16.9.9",
    "@types/react-syntax-highlighter": "^13.5.0",
    "@types/vscode": "^1.48.0",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "css-loader": "^5.0.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.2",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.7",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0",
    "webpack": "^5.3.2",
    "webpack-cli": "^4.1.0",
    "yarn-run-all": "^3.1.1"
  },
  "dependencies": {
    "@types/astring": "^1.3.0",
    "@typescript-eslint/parser": "^4.14.0",
    "acorn": "^8.0.4",
    "astring": "^1.6.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-syntax-highlighter": "^15.3.0"
  }
}
